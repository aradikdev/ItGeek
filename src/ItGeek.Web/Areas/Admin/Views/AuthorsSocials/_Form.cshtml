@model ItGeek.DAL.Entities.AuthorsSocial

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">
    <label asp-for="AuthorId" class="control-label"></label>
    <select asp-for="AuthorId" class="form-control" asp-items="ViewBag.AuthorId"></select>
    <span asp-validation-for="AuthorId" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="SocialLink" class="control-label"></label>
    <input asp-for="SocialLink" class="form-control" />
    <span asp-validation-for="SocialLink" class="text-danger"></span>
</div>
<input type="hidden" asp-for="Id" />
<div class="form-group">
    <input type="submit" value="Save" class="btn btn-primary" />
</div>